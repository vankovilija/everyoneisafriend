package  {		import flash.display.MovieClip;	import flash.display.StageScaleMode;	import flash.external.ExternalInterface;			public class Main extends MovieClip {						public function Main() {									stage.scaleMode = StageScaleMode.SHOW_ALL;						if (ExternalInterface.available) {  				ExternalInterface.call("OnRegisterSWFCallback", this);			}						trace("FLASH IS TRACING");		}	}	}