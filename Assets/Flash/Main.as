package  {		import flash.display.MovieClip;	import flash.display.StageScaleMode;	import flash.external.ExternalInterface;			import scaleform.clik.layout.LayoutData;	import scaleform.clik.core.UIComponent;	import scaleform.clik.layout.Layout;	import flash.display.StageAlign;		public class Main extends MovieClip {				public var layoutContainer:LayoutContainer;									public function Main() {									stage.scaleMode = StageScaleMode.NO_SCALE;			stage.align = StageAlign.TOP_LEFT;						if (ExternalInterface.available) {  				ExternalInterface.call("OnRegisterSWFCallback", this);			}					}								public function pickupScroll(scrollID:int):void		{			layoutContainer.pickupScroll(scrollID);		}	}	}