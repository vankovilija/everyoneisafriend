package  {		import flash.display.MovieClip;	import flash.display.StageScaleMode;	import flash.events.Event;	import flash.external.ExternalInterface;			public class MainMenuMain extends MovieClip {		public var mainMenu:MovieClip;				public function MainMenuMain() {			stage.scaleMode = StageScaleMode.SHOW_ALL;						addEventListener(Event.ENTER_FRAME, init);		}				private function init(e:Event):void		{			removeEventListener(Event.ENTER_FRAME, init);						if(ExternalInterface.available){				ExternalInterface.call("OnRegisterSWFCallback", this);			}						mainMenu.addEventListener(Event.COMPLETE, onMenuReady);		}				private function onMenuReady(e:Event):void		{			ExternalInterface.call("MenuReady");		}	}	}